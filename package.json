  "bugs": {
    "url": "https://github.com/ultimatehecker/Cybernetic/issues"
  },
  "homepage": "https://github.com/ultimatehecker/Cybernetic#readme",
  "dependencies": {
    "@sentry/node": "^7.26.0",
    "@sentry/tracing": "^7.21.1",
    "axios": "^1.2.0",
    "comma-number": "^2.1.0",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.2",
    "fs": "^0.0.1-security",
    "hypixel-api-reborn": "^10.0.0",
    "libsodium-wrappers": "^0.7.10",
    "minecraft-data": "^3.18.0",
    "mineflayer": "^4.5.1",
